[core]
s0_active_plugins = core;composite;opengl;imgpng;shift;move;put;gnomecompat;snap;mousepoll;place;compiztoolbox;screenshot;session;resize;grid;vpswitch;regex;animation;expo;wall;workarounds;ezoom;fade;scale;
s0_close_window_key = <Control><Primary>q
s0_maximize_window_key = <Super>Up
s0_unmaximize_window_key = <Super>Down
s0_show_desktop_key = <Super>d
s0_hsize = 4
s0_vsize = 1

[resize]
s0_initiate_key = <Super>r

[rotate]
s0_rotate_left_key = <Super>Left
s0_rotate_right_key = <Super>Right
s0_rotate_left_window_key = <Shift><Control><Primary>Left
s0_rotate_right_window_key = <Shift><Control><Primary>Right
s0_rotate_to_5_window_key = Control_L

[shift]
s0_next_key = <Alt>Tab

[move]
s0_initiate_key = <Super>m

[put]
s0_put_left_key = <Shift><Control><Primary><Super>Left
s0_put_right_key = <Shift><Control><Primary><Super>Right

[expo]
s0_expo_key = <Super>Tab
s0_deform = 2

[wall]
s0_left_key = <Super>Left
s0_right_key = <Super>Right
s0_left_window_key = <Shift><Control><Primary>Left
s0_right_window_key = <Shift><Control><Primary>Right

[gnomecompat]
s0_run_command_terminal_key = <Control><Primary>grave

